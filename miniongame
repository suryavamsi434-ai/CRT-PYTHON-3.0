   #minion_game
def minion_game(string):
    k=0
    s=0
    v="AEIOU"
    for i in range(len(string)):
        if string[i] in v:
            k+=len(string)-i
        else:
            s+=len(string)-i
    if k>s:
        print("kev in",k)
    else s>k:
        print("stuart",s)
    else:
        print("draw') 
    string="banana"
    minion_game(string)
    
